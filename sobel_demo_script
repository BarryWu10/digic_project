set exclude_gates {PadInC PadOut}
load_library /tools/mentor/adk/technology/leonardo/tsmc018_typ.syn
read {sobel_derivative.vhd sobel_eval.vhd sobel_demo.vhd}
ungroup -all -hier
set sdf_write_flat_netlist TRUE
optimize
write -format vhdl sobel_demo_1.vhd
write -format sdf sobel_demo_1.sdf
write -format verilog sobel_demo_1.v
